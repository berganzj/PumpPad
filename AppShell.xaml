<?xml version="1.0" encoding="UTF-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:PumpPad"
       x:Class="PumpPad.AppShell">

    <TabBar>
        <Tab Title="New Workout">
            <ShellContent ContentTemplate="{DataTemplate local:NewWorkoutPage}" />
        </Tab>
        <Tab Title="Review Workouts">
            <ShellContent ContentTemplate="{DataTemplate local:ReviewWorkoutsPage}" />
        </Tab>
    </TabBar>

</Shell>